class MinStack {

    public MinStack() {
         stack<int>stk;
    }
    
    public void push(int val) {
        stk.push(val);
        return;
    }
    
    public void pop() {
        if(!stk.empty())
        {
            stk.pop();
        }
    }
    
    public int top() {
         if(!stk.empty())
        {
            stk.top();
        }
     }
    
    public int getMin() {
    
    }
};

